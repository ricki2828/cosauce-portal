    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
